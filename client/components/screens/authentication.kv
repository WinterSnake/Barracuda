#: kivy 2.2.1

<AuthenticationScreen>:
	GridLayout:
		cols: 2
		rows: 3
		TextInput:
			hint_text: "Username"
			multiline: False
		TextInput:
			hint_text: "Password"
			multiline: False
		TextInput:
			hint_text: "IP"
			multiline: False
		TextInput:
			hint_text: "Port"
			multiline: False
		Button:
			text: "Login"
		Button:
			text: "Register"
